config_version=4

_global_script_classes=[  ]
_global_script_class_icons={

}

[application]

config/name="PixelCollabProject"
run/main_scene="res://UI/Menus/StartupMenu/StartupMenu.tscn"
config/icon="res://icon.png"

[autoload]

SceneSwitcher="*res://Engine/SceneSwitcher/SceneSwitcher.tscn"
Items="*res://Engine/Items.gd"
DebugConsole="*res://Engine/DEBUG/DebugConsole.tscn"
SaveLoadVariables="*res://Engine/SaveLoadVariables.gd"
SystemTimeManagement="*res://Engine/SystemTimeManagement.gd"
GeneralPlayerUI="*res://UI/GeneralPlayerUI/GeneralPlayerUI.tscn"
PredefinedColors="*res://Engine/PredefinedColors.gd"
Conversations="*res://Engine/Conversations.gd"

[display]

window/size/width=640
window/size/height=360
window/size/fullscreen=true
window/size/always_on_top=true
window/stretch/mode="2d"
window/stretch/aspect="keep"

[importer_defaults]

texture={
